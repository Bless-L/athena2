#!/usr/bin/env node

const program = require('commander')

const {
  printAthenaVersion
} = require('../src/util')

printAthenaVersion()

program
  .option('--debug', 'Debug info')
  .parse(process.argv)

const args = program.args
const { debug } = program

const serve = require('../src/build/serve')
serve(args, { debug })
