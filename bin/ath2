#!/usr/bin/env node

const program = require('commander')

const {
  getAthenaVersion
} = require('../src/util')

program
  .version(getAthenaVersion())
  .usage('<command> [options]')

program
  .command('init [url]', 'Init Athena')
  .command('app [appName]', 'Create an app')
  .parse(process.argv)
